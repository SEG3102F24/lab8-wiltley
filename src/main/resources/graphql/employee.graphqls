
type Employee {
    id: ID!
    name: String!
    dateOfBirth: String!
    city: String!
    salary: Float!
    gender: String
    email: String
}

input CreateEmployeeInput {
    name: String!
    dateOfBirth: String!
    city: String!
    salary: Float!
    gender: String
    email: String
}

type Query {
    employees: [Employee]
}

type Mutation {
    addEmployee(input: CreateEmployeeInput!): Employee
}
